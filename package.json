    "@mui/material": "^5.15.10",
    "@prisma/client": "^6.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.5.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^11.1.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.19.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^9.0.8",
    "prisma": "^6.6.0",
    "react-scripts": "5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:server": "tsc -p tsconfig.server.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server:dev": "ts-node src/server/index.ts",
    "server:prod": "node dist/server/index.js",
    "prisma:deploy": "prisma migrate deploy",
    "prod:prepare": "npm run build && npm run build:server && npm run prisma:deploy",
    "prod:start": "pm2 start dist/server/index.js --name cloud-subnets-tracker",
    "prod:stop": "pm2 stop cloud-subnets-tracker",
    "prod:restart": "pm2 restart cloud-subnets-tracker",
    "prod:logs": "pm2 logs cloud-subnets-tracker",
    "lint": "eslint src/**/*.{ts,tsx} --fix",
    "deploy": "npx prisma generate && npx prisma db push"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
